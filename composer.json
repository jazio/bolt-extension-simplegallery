{
    "name": "jazio/bolt-extension-simplegallery",
    "description": "A simple gallery based on file folder",
    "type": "bolt-extension",
    "keywords": [
        "Images",
        "Photos",
        "Gallery"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jazio",
            "email": "spomello@yandex.com"
        }
    ],
    "minimum-stability": "1.0.*",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Jazio\\SimpleGallery\\": "src"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Jazio\\SimpleGallery\\SimpleGallery",
              "branch-alias": {
                "dev-master": "1.0.0"
             },
        "bolt-icon": "gallery.png"
    }
}
